IOBUF PORT "o_tx_datavalid" IO_TYPE=LVDS ;
IOBUF PORT "o_housekeeping_miso" IO_TYPE=LVDS DIFFRESISTOR=OFF DRIVE=NA DIFFDRIVE="3.5" ;
IOBUF PORT "i_housekeeping_mosi" IO_TYPE=LVDS DIFFRESISTOR=100 ;
IOBUF PORT "i_housekeeping_ce" IO_TYPE=LVDS DIFFRESISTOR=100 TERMINATION=OFF ;
IOBUF PORT "o_housekeeping_dout[0]" IO_TYPE=LVCMOS33 PULLMODE=NONE ;
IOBUF PORT "o_housekeeping_dout[1]" IO_TYPE=LVCMOS33 PULLMODE=NONE ;
IOBUF PORT "o_housekeeping_dout[2]" IO_TYPE=LVCMOS33 ;
IOBUF PORT "o_housekeeping_dout[3]" IO_TYPE=LVCMOS33 ;
IOBUF PORT "o_housekeeping_dout[4]" IO_TYPE=LVCMOS33 DIFFRESISTOR=OFF ;
IOBUF PORT "o_housekeeping_dout[5]" IO_TYPE=LVCMOS33 ;
