IOBUF PORT "i_data_in[11]" DIFFRESISTOR=100 IO_TYPE=LVDS ;
# Control interface
IOBUF PORT "i_slow_clk" IO_TYPE=LVDS DIFFRESISTOR=100 ;
IOBUF PORT "i_trigger" IO_TYPE=LVDS DIFFRESISTOR=100 ;
IOBUF PORT "i_rst" IO_TYPE=LVCMOS33 ;
IOBUF PORT "i_start_transfer" IO_TYPE=LVCMOS33 DIFFRESISTOR=OFF PULLMODE=UP CLAMP=ON OPENDRAIN=OFF DIFFDRIVE="NA" HYSTERESIS=ON TERMINATION=OFF ;
IOBUF PORT "o_tx_clk" IO_TYPE=LVDS ;
IOBUF PORT "o_tx_data[0]" IO_TYPE=LVDS ;
